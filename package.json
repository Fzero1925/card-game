{
  "name": "universal-web-deployer",
  "version": "1.0.0",
  "description": "通用Web应用一键部署工具，支持GitHub Pages和Vercel",
  "main": "universal-deployer/bin/cli.js",
  "scripts": {
    "deploy": "node universal-deployer/bin/cli.js",
    "deploy:github": "node universal-deployer/bin/cli.js github",
    "deploy:vercel": "node universal-deployer/bin/cli.js vercel",
    "deploy:all": "node universal-deployer/bin/cli.js github vercel",
    "deploy:init": "node universal-deployer/bin/cli.js --init",
    "setup": "node scripts/setup.js",
    "help": "node universal-deployer/bin/cli.js --help"
  },
  "keywords": [
    "deploy",
    "deployment",
    "github-pages",
    "vercel",
    "web",
    "static-site",
    "automation"
  ],
  "author": "Fzero",
  "license": "MIT",
  "dependencies": {
    "open": "^8.4.0"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/your-repo.git"
  },
  "bin": {
    "web-deploy": "./universal-deployer/bin/cli.js"
  },
  "files": [
    "universal-deployer/",
    "scripts/",
    "deploy.config.js",
    "README.md"
  ]
}